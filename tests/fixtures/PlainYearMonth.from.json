    {
      "file": "test/built-ins/Temporal/PlainYearMonth/from/argument-string-date-with-utc-offset.js",
      "assertions": [
        {
          "kind": "assertPlainYearMonth",
          "actual": "2019-12",
          "expected": {
            "year": 2019,
            "month": 12
          },
          "description": "\"2019-12[Africa/Abidjan]\" is a valid UTC offset with time for PlainYearMonth"
        },
        {
          "kind": "assertPlainYearMonth",
          "actual": "2019-12",
          "expected": {
            "year": 2019,
            "month": 12
          },
          "description": "\"2019-12[!Africa/Abidjan]\" is a valid UTC offset with time for PlainYearMonth"
        },
        {
          "kind": "assertPlainYearMonth",
          "actual": "2019-12",
          "expected": {
            "year": 2019,
            "month": 12
          },
          "description": "\"2019-12[u-ca=iso8601]\" is a valid UTC offset with time for PlainYearMonth"
        },
        {
          "kind": "assertPlainYearMonth",
          "actual": "2019-12",
          "expected": {
            "year": 2019,
            "month": 12
          },
          "description": "\"2019-12[Africa/Abidjan][u-ca=iso8601]\" is a valid UTC offset with time for PlainYearMonth"
        },
        {
          "kind": "assertPlainYearMonth",
          "actual": "2019-12",
          "expected": {
            "year": 2019,
            "month": 12
          },
          "description": "\"2019-12-15T00+00:00\" is a valid UTC offset with time for PlainYearMonth"
        },
        {
          "kind": "assertPlainYearMonth",
          "actual": "2019-12",
          "expected": {
            "year": 2019,
            "month": 12
          },
          "description": "\"2019-12-15T00+00:00[UTC]\" is a valid UTC offset with time for PlainYearMonth"
        },
        {
          "kind": "assertPlainYearMonth",
          "actual": "2019-12",
          "expected": {
            "year": 2019,
            "month": 12
          },
          "description": "\"2019-12-15T00+00:00[!UTC]\" is a valid UTC offset with time for PlainYearMonth"
        },
        {
          "kind": "assertPlainYearMonth",
          "actual": "2019-12",
          "expected": {
            "year": 2019,
            "month": 12
          },
          "description": "\"2019-12-15T00-02:30[America/St_Johns]\" is a valid UTC offset with time for PlainYearMonth"
        }
      ]
    },
