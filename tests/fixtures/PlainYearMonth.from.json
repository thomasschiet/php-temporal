    {
      "file": "test/built-ins/Temporal/PlainYearMonth/from/argument-plaindate.js",
      "assertions": [
        {
          "kind": "assertPlainYearMonth",
          "actual": "1976-11",
          "expected": {
            "year": 1976,
            "month": 11
          },
          "description": null
        },
        {
          "kind": "sameValue",
          "actual": "iso8601",
          "expected": "iso8601",
          "description": "calendar string should be iso8601"
        },
        {
          "kind": "sameValue",
          "actual": "1976-11-01[u-ca=iso8601]",
          "expected": "1976-11-01[u-ca=iso8601]",
          "description": "iso reference date"
        }
      ]
    },
