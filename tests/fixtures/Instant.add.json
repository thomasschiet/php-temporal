      "file": "test/built-ins/Temporal/Instant/prototype/add/cross-epoch.js",
      "assertions": [
        {
          "kind": "assertInstant",
          "actual": "1969-12-15T12:23:45.678900434Z",
          "expected": "1969-12-15T12:23:45.678900434Z",
          "description": "(1969-12-25T12:23:45.678901234Z).subtract({ hours: 240, nanoseconds: 800 }) = 1969-12-15T12:23:45.678900434Z"
        },
        {
          "kind": "assertInstant",
          "actual": "1970-01-04T12:23:45.678902034Z",
          "expected": "1970-01-04T12:23:45.678902034Z",
          "description": "(1969-12-25T12:23:45.678901234Z).add({ hours: 240, nanoseconds: 800 }) = 1970-01-04T12:23:45.678902034Z"
        },
        {
          "kind": "assertInstant",
          "actual": "1969-12-15T12:23:45.678900434Z",
          "expected": "1969-12-15T12:23:45.678900434Z",
          "description": "(1970-01-04T12:23:45.678902034Z).subtract({ hours: 480, nanoseconds: 1600 }) = 1969-12-15T12:23:45.678900434Z"
        },
        {
          "kind": "assertInstant",
          "actual": "1970-01-04T12:23:45.678902034Z",
          "expected": "1970-01-04T12:23:45.678902034Z",
          "description": "(1969-12-15T12:23:45.678900434Z).add({ hours: 480, nanoseconds: 1600 }) = 1970-01-04T12:23:45.678902034Z"
        }
      ]
    },
