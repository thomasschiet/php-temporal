      "file": "test/built-ins/Temporal/PlainDate/from/argument-leap-second.js",
      "assertions": [
        {
          "kind": "assertPlainDate",
          "actual": "2016-12-31",
          "expected": {
            "year": 2016,
            "month": 12,
            "day": 31
          },
          "description": "leap second is a valid ISO string for PlainDate"
        },
        {
          "kind": "assertPlainDate",
          "actual": "2016-12-31",
          "expected": {
            "year": 2016,
            "month": 12,
            "day": 31
          },
          "description": "leap second is a valid ISO string for PlainDate"
        },
        {
          "kind": "assertPlainDate",
          "actual": "2016-12-31",
          "expected": {
            "year": 2016,
            "month": 12,
            "day": 31
          },
          "description": "second: 60 is ignored in property bag for PlainDate"
        },
        {
          "kind": "assertPlainDate",
          "actual": "2016-12-31",
          "expected": {
            "year": 2016,
            "month": 12,
            "day": 31
          },
          "description": "second: 60 is ignored in property bag for PlainDate even with overflow: reject"
        }
      ]
    },
