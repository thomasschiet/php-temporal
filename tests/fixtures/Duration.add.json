          "description": "microseconds balance"
        },
        {
          "kind": "assertDuration",
          "actual": "PT1H1M0.999001001S",
          "expected": {
            "years": 0,
            "months": 0,
            "weeks": 0,
            "days": 0,
            "hours": 1,
            "minutes": 1,
            "seconds": 0,
            "milliseconds": 999,
            "microseconds": 1,
            "nanoseconds": 1
          },
          "description": "milliseconds balance"
        },
        {
          "kind": "assertDuration",
          "actual": "PT1H59.001001001S",
          "expected": {
            "years": 0,
            "months": 0,
            "weeks": 0,
            "days": 0,
            "hours": 1,
            "minutes": 0,
            "seconds": 59,
            "milliseconds": 1,
            "microseconds": 1,
            "nanoseconds": 1
          },
          "description": "seconds balance"
        },
        {
          "kind": "assertDuration",
          "actual": "PT59M1.001001001S",
          "expected": {
            "years": 0,
            "months": 0,
            "weeks": 0,
            "days": 0,
            "hours": 0,
            "minutes": 59,
            "seconds": 1,
            "milliseconds": 1,
