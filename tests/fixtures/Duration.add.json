{
  "type": "Duration",
  "method": "add",
  "source": "test/built-ins/Temporal/Duration/prototype/add",
  "cases": [
    {
      "file": "test/built-ins/Temporal/Duration/prototype/add/argument-string-fractional-units-rounding-mode.js",
      "assertions": [
        {
          "kind": "assertDuration",
          "actual": "PT1H1M52.5S",
          "expected": {
            "years": 0,
            "months": 0,
            "weeks": 0,
            "days": 0,
            "hours": 1,
            "minutes": 1,
            "seconds": 52,
            "milliseconds": 500,
            "microseconds": 0,
            "nanoseconds": 0
          },
          "description": "positive fractional units rounded with correct rounding mode"
        },
        {
          "kind": "assertDuration",
          "actual": "-PT1H1M52.5S",
          "expected": {
            "years": 0,
            "months": 0,
            "weeks": 0,
            "days": 0,
            "hours": -1,
            "minutes": -1,
            "seconds": -52,
            "milliseconds": -500,
            "microseconds": 0,
            "nanoseconds": 0
          },
          "description": "negative fractional units rounded with correct rounding mode"
        }
      ]
    },
    {
      "file": "test/built-ins/Temporal/Duration/prototype/add/argument-string.js",
      "assertions": [
        {
          "kind": "assertDuration",
          "actual": "P3DT10M",
          "expected": {
            "years": 0,
            "months": 0,
            "weeks": 0,
            "days": 3,
            "hours": 0,
            "minutes": 10,
            "seconds": 0,
            "milliseconds": 0,
            "microseconds": 0,
            "nanoseconds": 0
          },
          "description": "String argument should be supported"
        }
      ]
    },
