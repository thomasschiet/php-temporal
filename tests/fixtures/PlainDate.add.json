          "expected": {
            "year": 2001,
            "month": 6,
            "day": 1
          },
          "description": "endYesterdayNextDay"
        },
        {
          "kind": "sameValue",
          "actual": "30",
          "expected": "30",
          "description": null
        },
        {
          "kind": "assertPlainDate",
          "actual": "2000-12-01",
          "expected": {
            "year": 2000,
            "month": 12,
            "day": 1
          },
          "description": "startReverseNextDay"
        },
        {
          "kind": "sameValue",
          "actual": "30",
          "expected": "30",
          "description": "adding months should result in same day"
