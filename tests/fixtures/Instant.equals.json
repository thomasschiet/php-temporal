      "assertions": [
        {
          "kind": "sameValue",
          "actual": "true",
          "expected": "true",
          "description": "date-time + Z preserves exact time"
        },
        {
          "kind": "sameValue",
          "actual": "false",
          "expected": "false",
          "description": "date-time + offset preserves exact time with offset"
        },
        {
